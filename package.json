{
  "name": "@gabrielsens/uikit",
  "private": false,
  "version": "0.0.16",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "sideEffects": false,
  "main": "dist/src/index.js",
  "module": "dist/src/index.mjs",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/src/index.mjs",
      "require": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.mjs"
    },
    "./styles.css": "./dist/src/styles/index.css",
    "./utils": {
      "import": "./dist/src/utils.mjs",
      "require": "./dist/src/utils.js",
      "types": "./dist/src/utils.d.ts",
      "default": "./dist/src/utils.mjs"
    },
    "./tailwind-preset": {
      "import": "./dist/tailwind.config.mjs",
      "require": "./dist/tailwind.config.js",
      "types": "./dist/tailwind.config.d.ts",
      "default": "./dist/tailwind.config.mjs"
    }
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "build": "tsup",
    "build:production": "export NODE_ENV=production && yarn build",
    "build:watch": "tsup --watch"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.427.0",
    "tailwind-merge": "^2.4.0"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.2.8",
    "@storybook/addon-themes": "^8.2.8",
    "@storybook/blocks": "^8.2.8",
    "@storybook/react": "^8.2.8",
    "@storybook/react-vite": "^8.2.8",
    "@storybook/test": "^8.2.8",
    "@types/node": "^22.2.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "8",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-storybook": "^0.8.0",
    "globals": "^15.9.0",
    "postcss": "^8.4.41",
    "storybook": "^8.2.8",
    "tailwindcss": "^3.4.9",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.2.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.4.0"
  }
}
